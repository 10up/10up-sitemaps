{
  "name": "10up/tenup-sitemaps",
  "description": "10up Sitemaps",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Taylor Lovett",
      "email": "taylor.lovett@10up.com"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "TenupSitemaps\\": "includes/classes/"
    }
  },
  "require-dev": {
    "10up/phpcs-composer": "dev-master",
    "phpunit/phpunit": "^7"
  },
  "scripts": {
    "lint": "phpcs .",
    "lint-fix": "phpcbf .",
    "test": "phpunit",
    "setup-local-tests": "bash bin/install-wp-tests.sh ep_wp_test root password mysql latest true"
  }
}
